{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-var": [
            "error"
        ],
        "prefer-const": [
            "warn"
        ],
        "strict": [
            "error"
        ]
    }
}